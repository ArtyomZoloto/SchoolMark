<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		 http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <student.servlet>
        <student.servlet-name>director-test-list</student.servlet-name>
        <student.servlet-class>com.inno.studentmodule.servlet.DirectorTestListServlet</student.servlet-class>
    </student.servlet>
    <student.servlet>
        <student.servlet-name>director-test-view</student.servlet-name>
        <student.servlet-class>com.inno.studentmodule.servlet.DirectorTestViewServlet</student.servlet-class>
    </student.servlet>

    <student.servlet-mapping>
        <student.servlet-name>director-test-list</student.servlet-name>
        <url-pattern>/director-test-list</url-pattern>
    </student.servlet-mapping>
    <student.servlet-mapping>
        <student.servlet-name>director-test-view</student.servlet-name>
        <url-pattern>/director-test-view</url-pattern>
    </student.servlet-mapping>
</web-app>